						<l n="2">
							<seg type="word">ἰᾶ<supplied reason="lost">τ᾽</supplied></seg>
							<supplied reason="lost">Εὐρύπυλον βεβλημένον· οἳ δὲ μάχοντο</supplied>
						</l>
						<l n="3">
							<seg type="word">Ἀργ<unclear>ε</unclear><supplied reason="lost">ῖοι</supplied></seg>
							<supplied reason="lost">καὶ Τρῶες ὁμιλαδόν. οὐδ᾽ ἂρ ἔμελλε</supplied>
						</l>
						<l n="4">
							<seg type="word">τάφρ<supplied reason="lost">ος</supplied></seg>
							<supplied reason="lost">ἔτι σχήσειν Δαναῶν καὶ τεῖχος ὕπερθεν</supplied>
						</l>
						<l n="5">εὐρύ, <supplied reason="lost">τὸ ποιήσαντο νεῶν ὕπερ, ἀμφὶ δὲ τάφρον</supplied></l>
						<l n="6">
							<seg type="word">ἤ<unclear>λα</unclear><supplied reason="lost">σαν,</supplied></seg>
							<supplied reason="lost">οὐδὲ θεοῖσι δόσαν κλειτὰς ἑκατόμβας,</supplied>
						</l>
						<l n="7">
							<seg type="word">ὄ<unclear>φ</unclear>ρ<unclear>ά</unclear></seg>
							<supplied reason="lost">σφιν νῆάς τε θοὰς καὶ ληίδα πολλὴν</supplied>
